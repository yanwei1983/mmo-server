    "cmake.sourceDirectory": "${workspaceFolder}/server-code/",
    "cmake.buildDirectory": "${workspaceFolder}/server-code/build",
    "cmake.generator": "Ninja",
    "cmake.buildEnvironment": {
        "CXX": "clang++-9",
        "CC": "clang-9",
        "CCACHE_SLOPPINESS": "pch_defines,time_macros",
        "NINJA_STATUS": "\"[%p/%f/%t %e] \""
    },
    "cmake.configureEnvironment": {
        "USE_ASAN": "OFF",
        "USE_TSAN": "ON",
        "USE_UBSAN": "OFF",
        "TIDY": "OFF"
    },
    "C_Cpp.experimentalFeatures": "Enabled",
    "C_Cpp.intelliSenseMemoryLimit": 1024,
    "C_Cpp.intelliSenseCachePath": "${workspaceFolder}/.vscode/",
    "C_Cpp.intelliSenseCacheSize": 5120,
    "C_Cpp.vcpkg.enabled": false,
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "gitlens.codeLens.enabled":false,
    "gitlens.currentLine.enabled":false,
    "gitlens.hovers.enabled":false,
    "gitlens.toggleLineBlame": false,
    "git.autofetchPeriod": 360,
    "git.autorefresh": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.currentLine.details": false,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.hovers.pullRequests.enabled": false,
    "gitlens.currentLine.scrollable": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "lua.targetVersion": "5.3",
    "clangd.path": "/usr/bin/clangd-10",
    "clangd.arguments": [
        "--compile-commands-dir=/data/mmorpg/server-code/build",
        "--query-driver=/usr/bin/clang++-9",
        "--log=verbose"
    ],

    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "clang-format.executable": "clang-format-9",
    "clang.executable": "clang++-9",
    "clang.cxxflags": ["-std=c++17"],
    
